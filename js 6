const fruits = ['яблоко', 'персик', 'груша', 'ананас', 'банан']; 
const fruitsSort = fruits.sort(); 
console.log(fruits); 


//console.log(fruits.sort()); 
document.write('<ul>'); 
for(let i = 0; i <fruitsSort.length; i++){ 
    document.write('<li>'); 
    document.write(fruitsSort[i]); 
    document.write('</li>'); 
} 
document.write('</ul>')


